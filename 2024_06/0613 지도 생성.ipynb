   "source": [
    "import folium\n",
    "\n",
    "# 지도 생성\n",
    "mymap = folium.Map(location=[37.5665, 126.9780], zoom_start=12)\n",
    "\n",
    "# 마커 추가\n",
    "folium.Marker([37.5665, 126.9780], popup='Seoul').add_to(mymap)\n",
    "\n",
    "# HTML 파일로 저장\n",
    "mymap.save('mymap.html')"
   ]
